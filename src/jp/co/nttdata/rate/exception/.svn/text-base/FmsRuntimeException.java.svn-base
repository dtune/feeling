package jp.co.nttdata.rate.exception;

/**
 * FMSコアにおいて、算式解析不正で起こったシステムエラーである
 * <p>異常処理必要なし</P>
 * @author zhanghy
 *
 */
public class FmsRuntimeException extends RuntimeException {

	private static final long serialVersionUID = -2072857941816661275L;
	
	public FmsRuntimeException(String msg) {
		super(msg);
	}
	
	public FmsRuntimeException(String msg, Throwable t) {
		super(msg, t);
	}

	public FmsRuntimeException(Throwable t) {
		super(t);
	}	

}
