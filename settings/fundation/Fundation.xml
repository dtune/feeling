	 desc="解約返戻金抑制型医療保険">
		<FundationPattern>11_1</FundationPattern>
		<FundationPattern>12_1</FundationPattern>		
	</FundationGroup>
	
	<FundationGroup code="221" groupkey="${generation}${payment}"
		desc="七大生活習慣病特約">
		<FundationPattern>11_2</FundationPattern>
		<FundationPattern>12_2</FundationPattern>		
	</FundationGroup>
	
	<FundationGroup code="223" groupkey="${generation}${payment}"
		desc="三大疾病入院一時金特約">
	 	<FundationPattern>11_3</FundationPattern>
