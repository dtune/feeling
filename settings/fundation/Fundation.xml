	</FundationGroup>
	
	<FundationGroup code="225" groupkey="${generation}${payment}${qw}"
		desc="先進医療特約">
		<FundationPattern>11_1</FundationPattern>		
	</FundationGroup>

	<FundationGroup code="226" groupkey="${generation}${payment}${qw}"
		desc="特定在宅治療支援特約">
		<FundationPattern>13</FundationPattern>		
	</FundationGroup>
	
	<FundationGroup code="007,008,310" groupkey="${generation}${payment}"
	 desc="特定疾病保障定期保険と５年ごと利差配当付特定疾病保障終身保険">
		<FundationPattern>7</FundationPattern>	
	</FundationGroup>
	
	<FundationGroup code="031" groupkey="${generation}${payment}${qw}" desc="医療保険">              
       <FundationPattern std-xtime-fixed="true">1</FundationPattern>
       <FundationPattern std-xtime-fixed="true">2</FundationPattern>
       <FundationPattern>8_2</FundationPattern>                                                                                                                	                                                                                          
	</FundationGroup>  
	
	<!-- 203疾病入院特約 -->
	<FundationGroup code="203,206,208" groupkey="${generation}${payment}" desc="疾病入院特約">
       <FundationPattern std-xtime-fixed="true">1</FundationPattern>
       <FundationPattern>8_2</FundationPattern>			
	</FundationGroup>
	
	<!-- 205疾病退院後療養特約 -->
	<FundationGroup code="205" groupkey="${generation}${payment}" desc="疾病退院後療養特約">
		<FundationPattern std-xtime-fixed="true">1</FundationPattern>
