		<FundationPattern>10</FundationPattern>
	</FundationGroup>
	
	<FundationGroup code="276,278" groupkey="${generation}${payment}${qw}" desc="無事故給付金特約">
		<FundationPattern>1</FundationPattern>
		<FundationPattern>2</FundationPattern>
		<FundationPattern>9</FundationPattern>		
	</FundationGroup>	
	
	<FundationGroup code="309" groupkey="${generation}${payment}${dividend}" desc="特定疾病保障定期保険特約">
		<FundationPattern>7</FundationPattern>
	</FundationGroup>
	
	<FundationGroup code="301,302,303,304,308" groupkey="${generation}${payment}${dividend}" desc="逓減定期保険特約の計算基礎">
