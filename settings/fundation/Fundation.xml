	</FundationGroup>
	<FundationGroup code="307" groupkey="${generation}${payment}${dividend}" desc="生存給付金付定期保険特約の計算基礎">
		<FundationPattern>1</FundationPattern>
	</FundationGroup>
</Fundation-definition>
