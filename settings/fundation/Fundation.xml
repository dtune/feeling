	</FundationGroup>	

	<FundationGroup code="235,236,237,238" groupkey="${generation}${payment}${qw}" desc="退院後療養特約の計算基礎">
		<FundationPattern std-xtime-fixed="true">1</FundationPattern>
        <FundationPattern std-xtime-fixed="true">2</FundationPattern>
