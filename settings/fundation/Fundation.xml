		<Fundation name="7SH_Mt120" column="fund5" desc="給付日数120（発生率・解約率込み）"/>
		<Fundation name="7O_Mt" column="fund6"/>
	</FundationPattern>
	
	<FundationPattern ptn="12_3">	
		<Fundation name="Dt" column="fund1" desc="Dx=vx*ltx"/>
		<Fundation name="Nt" column="fund2"/>		
		<Fundation name="3SH_Mt" column="fund3"/>
	</FundationPattern>
	
	<FundationPattern ptn="13">
		<Fundation name="DT" column="fund1"/>
		<Fundation name="NT" column="fund2"/>		
		<Fundation name="HM_MT" column="fund3"/>
		<Fundation name="hm_rho" column="rou" desc="予定在宅治療発生率"/>
		<Fundation name="hm_qx" column="q" desc="予定広義死亡率"/>
	</FundationPattern>
	
	<!-- 
		これから商品毎の基数グループの定義である、商品の要件定義通りにそれぞれの基数作成方法を含む 
			1.nameは計算式定義に使われる；columnは応じるDBのコラム
			2.属性groupkeyでは、入力のレートキーより作ること、またPVHという基礎はデフォルトに付加するため、書く必要ない
			3.予定利率「rate」と現価率「v」は、特に書いて必要ない
	-->	
	
	<!-- qx=fund11,qw=fund12 -->
	<DefaultFundationGroup groupkey="${generation}${payment}" desc="デフォルト商品基数グループ">
		<FundationPattern>1</FundationPattern>		
	</DefaultFundationGroup>
	
	<FundationGroup code="009,209" groupkey="${generation}" desc="配当付こども保険">
		<FundationPattern>1</FundationPattern>
		<FundationPattern>009</FundationPattern>
	</FundationGroup>
	
	<FundationGroup code="016,017,030,029,311" groupkey="${generation}${payment}${qw}"
		desc="低W系商品">
		<FundationPattern>1</FundationPattern>
		<FundationPattern>2</FundationPattern>
	</FundationGroup>
	
	<FundationGroup code="261" groupkey="${generation}${payment}${qw}"
		desc="がん入院特約">
		<FundationPattern>1</FundationPattern>
		<FundationPattern>2</FundationPattern>
		<FundationPattern>8_2</FundationPattern>
	</FundationGroup>
	
	<FundationGroup code="027" groupkey="${generation}${payment}${qw}"
		desc="低解約返戻金型終身保険（無選択型）">
