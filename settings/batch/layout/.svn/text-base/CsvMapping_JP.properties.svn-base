########これから 計算のtype########
## special   normal    extend   paidup  partLumpSum   bringUp  deathBenefit###
batch.ratekey.calculate=extend

#####これから普通の場合############
ratekey.iCurrentDate.normal.label=当月経過年月
ratekey.t.transfer=iCurrentDate/100
ratekey.f.transfer=iCurrentDate%100
ratekey.t1.transfer=iCurrentDate/100
ratekey.f1.transfer=iCurrentDate%100

ratekey.deathIndex.label=特体死亡指数

ratekey.buzYearSign.label=事業年度末サイン

ratekey.periodKbn.label=保期区分CN

ratekey.paymentKbn.label=払期区分CN

ratekey.kaisu.label=払方CN

ratekey.payup.label=払込済種類CN

ratekey.payment.label=払込状態CN

ratekey.state.transfer=if((payup==1&&payment==2)||(payup==2&&payment==2)){2}\
                              elseIf(payup==6&&payment==2){3}\
                              elseIf(payup==0&&payment==1){1}\
                              elseIf(payup==4&&payment==2){4}\
                              else{5}

ratekey.keiro.label=P扱方法CN
ratekey.keiro.transfer=if(keiro==1){1}\
                        elseIf(keiro==2){3}\
                        elseIf(keiro==3){4}\
                        elseIf{keiro==4}{5}\
                        elseIf(keiro==6){2}\
                        else{0}

ratekey.ownersex.label=契約者性別
ratekey.contractorSex.label=契約者性別CN

ratekey.sex.label=被保性別CN
ratekey.sex.transfer=sex-1

ratekey.x.label=X年齢
ratekey.y.label=Y年齢

ratekey.Sy.label=ファミリー配偶者保障割合

ratekey.Sz.label=ファミリー子保障割合

ratekey.I.label=災疾保障期間
ratekey.contractorType.transfer=if(I==19){1}\
                               elseIf(I==21){2}\
                               elseIf(I==20){20}\
                               elseIf(I==22){4}\
                               else{0}

ratekey.step.label=修正払込軽減期間

ratekey.n.label=保期年
ratekey.n.transfer=if(periodKbn==2){n-x}\
                   elseIf(periodKbn==3){99}\
                   else{n}

ratekey.m.label=払期年
ratekey.m.transfer=if(paymentKbn==2){m-x}\
                    elseIf(paymentKbn==3){99}\
                    else{m}

ratekey.g.label=年金支払年
ratekey.g.transfer=if(annuityType==5){h}else{g}
ratekey.t2.transfer=t-n-l
ratekey.t3.transfer=t-n

ratekey.l.label=繰り下げ年数

ratekey.sptate.label=P建サイン

ratekey.VrateUnit.label=VTレート単位

ratekey.contractDate.label=始期年月日
ratekey.gen.transfer=if((annuityType==16||annuityType==17)&&kaisu==1){2}\
						elseIf(contractDate<=19990401){1}\
                        elseIf(contractDate<=20010401){2}\
                        elseIf(contractDate<=20070331){3}\
                        elseIf(contractDate<=20090401){4}\
                        elseIf(contractDate<=20100930&&annuityType==11){4}\
                        elseIf(contractDate<=20100930){5}\
                        else{6}

ratekey.k.label=年金支払満了期間
ratekey.k.transfer=if(periodKbn==2){k-x}else{k}

ratekey.h.label=年金支払保証年

ratekey.u.label=低解返期間
ratekey.u.transfer=if(periodKbn==2){u-m}else{m}

ratekey.i.label=料率識別区分CN

ratekey.kk1.label=料率識別区分CN
ratekey.k1.transfer=if(kk1==72){2}elseIf(kk1==73){3}else{0}

ratekey.aliveS.label=延長生存Ｓ

ratekey.f1.label=払期月

ratekey.WrateUnit.label=Wレート単位

ratekey.PrateUnit.label=Pレート単位

ratekey.SA.label=基準S

ratekey.CompareDt.label=当年度末未割当残高

ratekey.Dt_lastyear.label=前年度末未割当残高

ratekey.divEndDate.label=配当基準日

ratekey.contractUpdType.label=更新契約サイン

ratekey.annuityType.label=料率保種CN
ratekey.policyCode.label=基礎料率保種CN
ratekey.policyCode.transfer=if(policyCode==5){4}else{policyCode}
ratekey.dividend.label=予定利率識別CN
ratekey.dividend.transfer=if(dividend==0||dividend==2||dividend==3){0}\
							else{1}
ratekey.partOnetime.transfer=if(dividend==0||dividend==2||dividend==4){0}\
							else{1}

ratekey.contractorState.label=契約現況CN
ratekey.contractorState.transfer=if(contractorState==30){0}else{1}
ratekey.bonusDiv.transfer=0
ratekey.theta.transfer=0
ratekey.annuityY.transfer=if(periodKbn==2){n+l}else{x+n+l}
ratekey.accCalculation.transfer=0
ratekey.finalPercent.transfer=if(kk1==31){0}elseIf(kk1==34){1}else{2}
ratekey.teizoKbn.transfer=if(kk1==40){0}else{1}
ratekey.afterBenifitPay.transfer=0
ratekey.CompareAllocation.label=翌期配当所要額
ratekey.changeState.transfer=2
ratekey.SD.label=基礎契約払済時死亡給付金額
ratekey.paidupS.transfer=SA
ratekey.birthday.label=被保生年月日

batch.ratekey=gen,sex,kaisu,x,iCurrentDate,paymentKbn,payup,periodKbn,payment,contractUpdType,sptate,\
               n,m,g,state,keiro,SA,t,f,t1,f1,t2,t3,l,contractDate,k,h,u,step,k1,deathIndex,\
              Sy,Sz,i,contractorType,VrateUnit,buzYearSign,Wrate,WrateUnit,aliveS,PrateUnit,kk1,\
              CompareDt,Dt_lastyear,divEndDate,contractUpdType,annuityType,dividend,policyCode,\
              afterBenifitPay,y,contractorSex,CompareAllocation,SD,birthday

batch.ratekey.transfer=n,m,t,f,t1,f1,state,keiro,sex,contractorType,t2,t3,gen,k1,k,u,\
						contractorState,bonusDiv,theta,annuityY,accCalculation,g,dividend,\
						finalPercent,teizoKbn,afterBenifitPay,policyCode,partOnetime,changeState,paidupS
