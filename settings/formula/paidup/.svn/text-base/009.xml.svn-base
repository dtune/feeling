<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE Formula-definition SYSTEM "Formula-definition.dtd">
<!-- 商品毎に計算公式はここに定義する -->
<Formula-definition>

	<!-- 払済　ここから -->
	<Paidup desc="払済">
		<formula name="gamma_nashu_013" value="0.002"/>
		<formula name="PaidupV_013" paras="t,n" fraction="4">
        	A_x_n(x+t,n-t)+gamma_nashu_013*a_x_n(x+t,n-t)
		</formula>
		<formula name="Vrate_Paidup" fraction="4">
			(1-f_12(f))*PaidupV_013(t,m)+f_12(f)*PaidupV_013(t+1,m)
		</formula>

		<formula name="Vrate_PaidEnd_N" fraction="4">
			if(afterBenifitPay==0){
				sum(t,n-1){(1+gamma_2nashu)*tFx(index)*f_A_x_y_n@head(index-t,x+t,y+t,1)}
					+
				2*A_x_n_D(x+t,n-t)+gamma_nashu*a_x_y_n(x+t,y+t,n-t)
			}else{
				2*A_x_n_D(x+t,n-t)+gamma2*a_x_n(x+t,n-t)
			}
		</formula>
		<formula name="Vrate_PaidEnd" fraction="4">
			(1-f_12(f))*Vrate_PaidEnd_N(t)+f_12(f)*Vrate_PaidEnd_N(t+1)
		</formula>

		<formula name="paidup_Premium" pvh="P" accessable="true" desc="１．払済保険金額">
			roundUp(
				roundDown((SurrenderFeeS-L)/Vrate_Paidup,0)
			,0-3)
		</formula>

		<formula name="paidup_SurrenderFee" pvh="P" accessable="true" desc="２．解約払戻金">
			roundDown((SurrenderFeeS-L),0)
		</formula>
	</Paidup>
</Formula-definition>