<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RateKey-definition SYSTEM "RateKey-definition.dtd">
<RateKey-definition>
  	<!-- レートキーの制御関係 -->
  	<Constraints>
  		<Constraint keys="n,m" condition="(m&lt;99&amp;&amp;n>=m}||(m==99)"
  		 desc="保険期間は払込期間より大きいで入力ください。"/>
  	</Constraints>
  	  	
  	<!-- レートキーのUI属性 -->
    <RateKeys>
        <RateKey name="bringUpS" label="養育年金" type="text"
         required="true" initValue="1000000" displayOrder="80">
          <rule min="0" max="999999999" step="1"/>
        </RateKey>
    </RateKeys>
</RateKey-definition>